{
  "name": "neronplex/cake-orm-enabler",
  "description": "CakePHP ORM enabler for Laravel & Lumen",
  "type": "library",
  "keywords": ["lavavel", "lumen", "cakephp", "orm"],
  "homepage": "https://github.com/neronplex/cake-orm-enabler",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "暖簾 (@neronplex)",
      "email": "admin@neronplex.info",
      "homepage": "https://www.neronplex.jp/"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "^5.0,<5.4",
    "cakephp/orm": "^3.3"
  },
  "suggest": {
    "cakephp/cache": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Neronplex\\CakeORMEnabler\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Neronplex\\CakeORMEnabler\\Test\\": "tests"
    }
  }
}